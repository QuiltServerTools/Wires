{
  "schemaVersion": 1,
  "id": "wires",
  "version": "${version}",

  "name": "Wires",
  "description": "Server utilities for Quilt",
  "authors": [
    "yitzy299"
  ],
  "contact": {
    "homepage": "https://github.com/quiltservertools",
    "sources": "https://github.com/quiltservertools/wires"
  },

  "license": "MIT",

  "environment": "server",
  "entrypoints": {
    "server": [
      {
        "value": "net.quiltservertools.wires.Wires::INSTANCE"
      }
    ]
  },
  "mixins": [
    "wires.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.7",
    "fabric": ">=0.56.0",
    "minecraft": "1.19.x",
    "java": ">=17",
    "fabric-language-kotlin": "*"
  }
}
